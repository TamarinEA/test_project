<%= form_for(:test) do |f| %>
  <p><%= f.label :name %><%= f.file_field :name, :value => @file[:name] %></p>
  <p><%= f.label :word %><%= f.text_field :word, :value => @file[:word] %></p>
  <p><%= f.label :email %><%= f.email_field :email, :value => @file[:email] %></p>
  <p><%= f.submit %></p>
<% end %>
